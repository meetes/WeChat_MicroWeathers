/**app.wxss**/
@import "common/WeUi/weui.wxss";
page {
  font-family: "微软雅黑";
  background: #f4f6f9;
}

.container {
  color: #333;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  background: #f4f6f9;
  font-family: "微软雅黑";
}

.clear {
  clear: both;
  width: 100%;
}

.clear_5 {
  clear: both;
  height: 10rpx;
  width: 100%;
}

.clear_10 {
  clear: both;
  height: 20rpx;
  width: 100%;
}

.navigator-hover {
  background-color: rgba(0, 0, 0, 0);
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.loading {
  text-align: center;
  color: #999;
  font-size: 34rpx;
  line-height: 80rpx;
}
